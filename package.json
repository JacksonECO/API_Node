{
  "name": "api_node",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --silent --colors --passWithNoTests",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "jest --silent --colors --passWithNoTests --findRelatedTests",
    "test:ci": "npm test -- --coverage --noStackTrace"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JacksonECO/API_Node.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JacksonECO/API_Node/issues"
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "npm test:staged",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:ci"
    }
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "homepage": "https://github.com/JacksonECO/API_Node#readme",
  "devDependencies": {
    "husky": "^4.0.0-beta.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "standard": "^16.0.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "express": "^4.17.1",
    "mongoose": "^5.12.0"
  }
}
